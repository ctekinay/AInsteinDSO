default2:ServiceProvider a skos:Concept ;
    skos:altLabel "SP" ;
    skos:definition "Role: means a market participant which has or intends to have a legal or contractual obligation to supply local or balancing services from at least one SPU or SPG"@en ;
    skos:inScheme default2:Ontology ;
    skos:prefLabel "Service Provider"@en ;
    skos:topConceptOf default2:Ontology .

default2:ServiceProvidingGroup a skos:Concept ;
    skos:altLabel "SPG" ;
    skos:definition "An aggregation of units and/or service-providing units connected to more than one connection point where they inject and/or withdraw electricity fulfilling the requirements to provide SO services either at the connection point or aggregated level."@en ;
    skos:inScheme default2:Ontology ;
    skos:prefLabel "Service Providing Group"@en ;
    skos:topConceptOf default2:Ontology .

default2:ServiceProvidingUnit a skos:Concept ;
    skos:altLabel "SPU" ;
    skos:definition "A unit or ensemble of units connected to a single connection point of a SO network where they inject and/or withdraw electricity fulfilling the requirements to provide SO services. "@en ;
    skos:inScheme default2:Ontology ;
    skos:prefLabel "Service Providing Unit"@en ;
    skos:topConceptOf default2:Ontology .

default2:TechnicalResource a skos:Concept ;
    skos:definition "means a distributed generation resource or an individual energy storage unit, or a demand unit or any other consumption device"@en ;
    skos:inScheme default2:Ontology ;
    skos:prefLabel "Technical Resource"@en ;
    skos:topConceptOf default2:Ontology .

