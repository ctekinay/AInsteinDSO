    skos:inScheme default13:Ontology ;
    skos:prefLabel "Charging Station"@en ;
    skos:topConceptOf default13:Ontology .

default13:96 a skos:Concept ;
    dc:source "IEC 61968-1" ;
    dct:identifier "urn:uuid:4f23391b-b464-5aa0-bde6-92983586dcd1" ;
