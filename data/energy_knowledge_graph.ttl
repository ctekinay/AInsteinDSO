default13:106 a skos:Concept ;
    dc:source "IEC 60050 (IEV 151-12-12)" ;
    dct:identifier "urn:uuid:8cedf8d0-4909-5019-88ac-f0d904c0bf04" ;
    rdfs:seeAlso <https://electropedia.org/iev/iev.nsf/display?openform&ievref=151-12-12> ;
    owl:versionInfo "1" ;
    skos:definition "Conductive part of a device, intended for connection to external circuits."@en,
        "Geleidend deel van een apparaat, bedoeld voor aansluiting op externe circuits."@nl ;
    skos:inScheme default13:Ontology ;
    skos:narrowMatch default6:Terminal ;
    skos:note "This means a terminal is a point of electrical connection — a physical conductive component designed to connect a wire, a busbar, or another piece of equipment — allowing the flow of current into or out of the apparatus."@en ;
    skos:prefLabel "Terminal"@en,
        "Terminal"@nl ;
    skos:topConceptOf default13:Ontology ;
    foaf:homepage <https://alliander.atlassian.net/l/cp/duZf6PeN> .

default13:108 a skos:Concept ;
    dc:source "IEC 60050 (IEV 351-41-24)" ;
    dct:identifier "urn:uuid:8cedf8d0-4909-5019-88ac-f0d904c0bf04" ;
