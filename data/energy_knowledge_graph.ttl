    skos:example "Adjusting the forecasted energy load on a EnergyConsumer based on a settlement condition."@en ;
    skos:inScheme default13:Ontology ;
    skos:prefLabel "Control Action"@en,
        "Controle Actie"@nl ;
    skos:related default13:102,
        default13:98 ;
    skos:topConceptOf default13:Ontology .

default13:103 a skos:Concept ;
